#################################################################
#    Specimen Roles
#################################################################

dyn:CharacterizationSpecimen rdf:type owl:NamedIndividual, dyn:SpecimenRole ;
    rdfs:label "Characterization Specimen"@en ;
    dyn:hasName "Characterization Specimen"@en ;
    dyn:hasDescription "Specimen used for material characterization (microstructure, phases, etc.)"@en .

dyn:TestSpecimen rdf:type owl:NamedIndividual, dyn:SpecimenRole ;
    rdfs:label "Test Specimen"@en ;
    dyn:hasName "Test Specimen"@en ;
    dyn:hasDescription "Specimen used for mechanical testing only"@en .

dyn:CalibrationSpecimen rdf:type owl:NamedIndividual, dyn:SpecimenRole ;
    rdfs:label "Calibration Specimen"@en ;
    dyn:hasName "Calibration Specimen"@en ;
    dyn:hasDescription "Specimen used for equipment calibration and validation"@en .

dyn:ReferenceSpecimen rdf:type owl:NamedIndividual, dyn:SpecimenRole ;
    rdfs:label "Reference Specimen"@en ;
    dyn:hasName "Reference Specimen"@en ;
    dyn:hasDescription "Specimen kept as reference for comparison or future analysis"@en .

#################################################################
#    Specimen Shapes
#################################################################

dyn:CylindricalShape rdf:type owl:NamedIndividual, dyn:Shape ;
    rdfs:label "Cylindrical Shape"@en ;
    dyn:hasName "Cylindrical"@en ;
    dyn:hasDescription "Right circular cylinder geometry for compression/tension testing"@en .

dyn:DogboneShape rdf:type owl:NamedIndividual, dyn:Shape ;
    rdfs:label "Dogbone Shape"@en ;
    dyn:hasName "Dogbone"@en ;
    dyn:hasDescription "ASTM standard tensile test specimen geometry with gauge section"@en .

dyn:CubicShape rdf:type owl:NamedIndividual, dyn:Shape ;
    rdfs:label "Cubic Shape"@en ;
    dyn:hasName "Cubic"@en ;
    dyn:hasDescription "Cubic geometry for lattice structure testing"@en .

#################################################################
#    Structure Types
#################################################################

dyn:MonolithicMaterial rdf:type owl:NamedIndividual, dyn:Structure ;
    rdfs:label "Monolithic Material"@en ;
    dyn:hasName "Monolithic"@en ;
    dyn:hasDescription "Homogeneous monolithic material structure without internal features"@en .

dyn:LatticeStructure rdf:type owl:NamedIndividual, dyn:Structure ;
    rdfs:label "Lattice Structure"@en ;
    dyn:hasName "Lattice"@en ;
    dyn:hasDescription "Periodic lattice or cellular structure with designed porosity"@en .

#################################################################
#    Lattice Types
#################################################################

dyn:GyroidLattice rdf:type owl:NamedIndividual, dyn:LatticeType ;
    rdfs:label "Gyroid Lattice"@en ;
    dyn:hasName "Gyroid"@en ;
    dyn:hasDescription "Triply periodic minimal surface (TPMS) lattice with gyroid geometry"@en .

dyn:SimpleCubicLattice rdf:type owl:NamedIndividual, dyn:LatticeType ;
    rdfs:label "Simple Cubic Lattice"@en ;
    dyn:hasName "Simple Cubic"@en ;
    dyn:hasDescription "Basic cubic lattice structure with orthogonal struts"@en .

#################################################################
#    Manufacturing Methods
#################################################################

dyn:3DPrinting rdf:type owl:NamedIndividual, dyn:ManufacturingMethod ;
    rdfs:label "3D Printing/AM"@en ;
    dyn:hasName "Additive Manufacturing"@en ;
    dyn:hasDescription "Layer-by-layer additive manufacturing process"@en .

dyn:Machining rdf:type owl:NamedIndividual, dyn:ManufacturingMethod ;
    rdfs:label "Machining"@en ;
    dyn:hasName "CNC Machining"@en ;
    dyn:hasDescription "Computer-controlled machining to final dimensions"@en .

dyn:Casting rdf:type owl:NamedIndividual, dyn:ManufacturingMethod ;
    rdfs:label "Casting"@en ;
    dyn:hasName "Casting"@en ;
    dyn:hasDescription "Process of creating an object by pouring a liquid material into a mold"@en .

#################################################################
#    Surface Finishes
#################################################################

dyn:AsReceivedFinish rdf:type owl:NamedIndividual, dyn:SurfaceFinish ;
    rdfs:label "As-Received"@en ;
    dyn:hasName "As-Received"@en ;
    dyn:hasDescription "Surface finish as received from manufacturing without additional processing"@en .

dyn:LappedFinish rdf:type owl:NamedIndividual, dyn:SurfaceFinish ;
    rdfs:label "Lapped"@en ;
    dyn:hasName "Lapped"@en ;
    dyn:hasDescription "Surface finish achieved through lapping process"@en .

dyn:PolishedFinish rdf:type owl:NamedIndividual, dyn:SurfaceFinish ;
    rdfs:label "Polished"@en ;
    dyn:hasName "Polished"@en ;
    dyn:hasDescription "Surface finish achieved through polishing process"@en .

#################################################################
#    Polishing Compounds
#################################################################

dyn:AluminaCompound rdf:type owl:NamedIndividual, dyn:PolishingCompound ;
    rdfs:label "Alumina"@en ;
    dyn:hasName "Alumina (Al₂O₃)"@en ;
    dyn:hasDescription "Aluminum oxide abrasive compound for general polishing and surface preparation"@en .

dyn:DiamondPolycrystallineCompound rdf:type owl:NamedIndividual, dyn:PolishingCompound ;
    rdfs:label "Diamond Polycrystalline"@en ;
    dyn:hasName "Diamond Polycrystalline Solution"@en ;
    dyn:hasDescription "Polycrystalline diamond suspension for fine polishing and mirror finish"@en .

dyn:SilicaCarbide rdf:type owl:NamedIndividual, dyn:PolishingCompound ;
    rdfs:label "Silicon Carbide"@en ;
    dyn:hasName "Silicon Carbide (SiC)"@en ;
    dyn:hasDescription "Silicon carbide abrasive for aggressive material removal and rough polishing"@en .

#################################################################
#    Heat Treatments
#################################################################

dyn:AsReceivedHT rdf:type owl:NamedIndividual, dyn:HeatTreatment ;
    rdfs:label "As-Received"@en ;
    dyn:hasName "As-Received"@en ;
    dyn:hasDescription "No heat treatment applied - as received from manufacturing"@en .

dyn:AnnealedHT rdf:type owl:NamedIndividual, dyn:HeatTreatment ;
    rdfs:label "Annealed"@en ;
    dyn:hasName "Annealed"@en ;
    dyn:hasDescription "Heat treatment to relieve internal stresses and soften material"@en .

dyn:FurnaceCooledHT rdf:type owl:NamedIndividual, dyn:HeatTreatment ;
    rdfs:label "Furnace Cooled"@en ;
    dyn:hasName "Furnace Cooled"@en ;
    dyn:hasDescription "Slow cooling within furnace for controlled phase transformation"@en .

dyn:AirCoolHT rdf:type owl:NamedIndividual, dyn:HeatTreatment ;
    rdfs:label "Air Cool"@en ;
    dyn:hasName "Air Cool"@en ;
    dyn:hasDescription "Cooling by exposure to ambient air"@en .

dyn:WaterQuenchHT rdf:type owl:NamedIndividual, dyn:HeatTreatment ;
    rdfs:label "Water Quench"@en ;
    dyn:hasName "Water Quench"@en ;
    dyn:hasDescription "Rapid cooling by immersion in water for maximum hardening"@en .

dyn:OilQuenchHT rdf:type owl:NamedIndividual, dyn:HeatTreatment ;
    rdfs:label "Oil Quench"@en ;
    dyn:hasName "Oil Quench"@en ;
    dyn:hasDescription "Moderate cooling by immersion in oil for controlled hardening"@en .

