dyn:CustomOrientation rdf:type owl:NamedIndividual, dyn:BuildOrientation ;
    rdfs:label "Custom Orientation"@en ;
    dyn:hasName "Custom Orientation"@en ;
    dyn:hasDescription "Custom build orientation - specify angle in processing notes"@en .

