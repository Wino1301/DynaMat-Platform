# This ontology extends the core DynaMat ontology with mechanical testing specific properties
# Import: Load DynaMat_core.ttl first in Protégé, then load this file

#################################################################
#    Test Type and Classification
#################################################################

dyn:hasTestType rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:MechanicalTest ;
    rdfs:range xsd:string ;
    rdfs:label "Test Type"@en ;
    dyn:hasDisplayName "Test Type" ;
    dyn:hasFormGroup "Identification" ;
    dyn:hasDisplayOrder 5 ;
    rdfs:comment "Type of test performed: calibration, elastic, or specimen test"@en .

#################################################################
