#################################################################
#    1. Identification Properties
#################################################################

dyn:hasSpecimenBatchID rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Specimen ;
    rdfs:range dyn:SpecimenBatchID ;
    rdfs:label "Specimen Batch ID"@en ;
    gui:hasDisplayName "Batch ID" ;
    gui:hasFormGroup "Identification" ;
    gui:hasGroupOrder 1 ;
    gui:hasDisplayOrder 2 ;
    rdfs:comment "Batch identifier for specimens from same processing run"@en .

dyn:hasBatchID rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:SpecimenBatchID ;
    rdfs:range xsd:string ;
    rdfs:label "Batch ID String"@en ;
    gui:hasDisplayName "Batch ID" ;
    rdfs:comment "String identifier for the batch (e.g., BATCH-AL001-2024-01)"@en .

dyn:hasSpecimenRole rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Specimen ;
    rdfs:range dyn:SpecimenRole ;
    rdfs:label "Specimen Role"@en ;
    gui:hasDisplayName "Specimen Role" ;
    gui:hasFormGroup "Identification" ;
    gui:hasGroupOrder 1 ;
    gui:hasDisplayOrder 3 ;
    rdfs:comment "Purpose of specimen in testing"@en .

