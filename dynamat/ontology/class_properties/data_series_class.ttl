#################################################################
#    Classes
#################################################################

###  https://dynamat.utep.edu/ontology#AnalysisFile
dyn:AnalysisFile rdf:type owl:Class ;
                 rdfs:label "Analysis File"@en ;
                 rdfs:comment "Container for analysis result data files with processing metadata"@en .


###  https://dynamat.utep.edu/ontology#DataSeries
dyn:DataSeries rdf:type owl:Class ;
               rdfs:label "Data Series"@en ;
               rdfs:comment "A series of measured or calculated data points representing a physical quantity from a mechanical test. Each series contains metadata about the data location (file, column), physical units, analysis method, and summary statistics."@en .


###  https://dynamat.utep.edu/ontology#ProcessedData
dyn:ProcessedData rdf:type owl:Class ;
                  rdfs:subClassOf dyn:DataSeries ;
                  rdfs:comment "Calculated quantities (stress, strain, force)"@en ;
                  rdfs:label "Processed Data"@en .


###  https://dynamat.utep.edu/ontology#RawSignal
dyn:RawSignal rdf:type owl:Class ;
              rdfs:subClassOf dyn:DataSeries ;
              rdfs:comment "Raw sensor voltage/signal data"@en ;
              rdfs:label "Raw Signal"@en .


###  https://dynamat.utep.edu/ontology#MechanicalTest
dyn:MechanicalTest rdf:type owl:Class .


###  https://dynamat.utep.edu/ontology#MeasurementEquipment
dyn:MeasurementEquipment rdf:type owl:Class .


###  https://dynamat.utep.edu/ontology#PulseShift
dyn:PulseShift rdf:type owl:Class .


###  https://dynamat.utep.edu/ontology#PulseDetectionParams
dyn:PulseDetectionParams rdf:type owl:Class .


###  https://dynamat.utep.edu/ontology#SeriesType
dyn:SeriesType rdf:type owl:Class ;
               rdfs:label "Series Type"@en ;
               rdfs:comment "Type of physical quantity represented by a data series (e.g., stress, strain, force, displacement)"@en .


