# Incident Bar Strain Gauges
dyn:StrainGauge_SHPB_001 rdf:type owl:NamedIndividual, dyn:StrainGauge ;
    rdfs:label "Incident Bar Strain Gauge SG1"@en ;
    dyn:hasName "Incident Bar Strain Gauge SG1"@en ;
    dyn:hasGaugeResistance 120.0 ;    # Ohms
    dyn:hasGaugeFactor 2.06 ;
    dyn:hasCalibrationVoltage 2.0 ;
    dyn:hasCalibrationResistance 59950.0 ;
    dyn:hasDistanceFromSpecimen 1219.2 ;  # mm
    dyn:hasInstallationDate "2024-01-15"^^xsd:date ;
    dyn:hasDescription "Strain gauge on incident bar for pulse measurement"@en .

dyn:StrainGauge_SHPB_002 rdf:type owl:NamedIndividual, dyn:StrainGauge ;
    rdfs:label "Incident Bar Strain Gauge SG2"@en ;
    dyn:hasName "Incident Bar Strain Gauge SG2"@en ;
    dyn:hasGaugeResistance 120.0 ;
    dyn:hasGaugeFactor 2.06 ;
    dyn:hasCalibrationVoltage 2.0 ;
    dyn:hasCalibrationResistance 59950.0 ;
    dyn:hasDistanceFromSpecimen 914.4 ;  # mm - backup location
    dyn:hasInstallationDate "2024-01-15"^^xsd:date ;
    dyn:hasDescription "Strain gauge on transmitted bar for pulse measurement"@en .

