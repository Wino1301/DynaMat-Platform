@prefix dyn: <https://dynamat.utep.edu/ontology#> .
@prefix gui: <https://dynamat.utep.edu/ontology/gui#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix qkdv: <http://qudt.org/vocab/quantitykind/> .
@prefix dc: <http://purl.org/dc/terms/> .

# =============================================================================
# MATERIAL CLASS PROPERTIES
# For DynaMat Platform - Dynamic Materials Testing Ontology
# Properties only - no individual instances
#
# All measurement properties use qudt:QuantityValue range to produce
# self-describing blank nodes with numeric value, unit, quantity kind,
# and optional uncertainty/provenance.
# =============================================================================

#################################################################
#    Object Properties - Mechanical Properties
#################################################################

dyn:hasElasticModulus rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:ForcePerArea ;
    rdfs:label "Elastic Modulus"@en ;
    gui:hasDisplayName "Elastic Modulus" ;
    gui:hasFormGroup "ElasticProperties" ;
    gui:hasDisplayOrder 1 ;
    dyn:hasUnit "unit:MegaPA" ;
    rdfs:comment "Young's modulus of elasticity"@en .

dyn:hasPoissonsRatio rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Poisson's Ratio"@en ;
    gui:hasDisplayName "Poisson's Ratio" ;
    gui:hasFormGroup "ElasticProperties" ;
    gui:hasDisplayOrder 2 ;
    rdfs:comment "Poisson's ratio"@en .

dyn:hasYieldStrength rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:ForcePerArea ;
    rdfs:label "Yield Strength"@en ;
    gui:hasDisplayName "Yield Strength" ;
    gui:hasFormGroup "JohnsonCookParameters" ;
    gui:hasDisplayOrder 1 ;
    dyn:hasUnit "unit:MegaPA" ;
    rdfs:comment "Yield strength (Johnson-Cook A parameter)"@en .

dyn:hasHardeningConstant rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:ForcePerArea ;
    rdfs:label "Hardening Constant"@en ;
    gui:hasDisplayName "Hardening Constant" ;
    gui:hasFormGroup "JohnsonCookParameters" ;
    gui:hasDisplayOrder 2 ;
    dyn:hasUnit "unit:MegaPA" ;
    rdfs:comment "Hardening constant (Johnson-Cook B parameter)"@en .

dyn:hasHardeningExponent rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Hardening Exponent"@en ;
    gui:hasDisplayName "Hardening Exponent" ;
    gui:hasFormGroup "JohnsonCookParameters" ;
    gui:hasDisplayOrder 3 ;
    rdfs:comment "Hardening exponent (Johnson-Cook n parameter)"@en .

dyn:hasStrainRateCoefficient rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Strain Rate Coefficient"@en ;
    gui:hasDisplayName "Strain Rate Coefficient" ;
    gui:hasFormGroup "JohnsonCookParameters" ;
    gui:hasDisplayOrder 4 ;
    rdfs:comment "Strain rate coefficient (Johnson-Cook C parameter)"@en .

dyn:hasThermalSofteningCoefficient rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Thermal Softening Coefficient"@en ;
    gui:hasDisplayName "Thermal Softening Coefficient" ;
    gui:hasFormGroup "JohnsonCookParameters" ;
    gui:hasDisplayOrder 5 ;
    rdfs:comment "Thermal softening coefficient (Johnson-Cook m parameter)"@en .

#################################################################
#    Object Properties - Characterization and Phases
#################################################################

# Link to characterization tests performed on specimens of this material
dyn:hasCharacterization rdf:type owl:ObjectProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range dyn:CharacterizationTest ;
    rdfs:label "Characterization Test"@en ;
    gui:hasDisplayName "Characterization Tests" ;
    gui:hasFormGroup "Characterization" ;
    gui:hasDisplayOrder 1 ;
    rdfs:comment "Links to characterization tests performed on specimens of this material"@en .

# Phase relationships
dyn:hasPhase rdf:type owl:ObjectProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range dyn:Phase ;
    rdfs:label "Phase"@en ;
    gui:hasDisplayName "Material Phases" ;
    gui:hasFormGroup "Microstructure" ;
    gui:hasDisplayOrder 1 ;
    rdfs:comment "Phases present in the material"@en .

dyn:hasPrimaryPhase rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf dyn:hasPhase ;
    rdfs:domain dyn:Material ;
    rdfs:range dyn:Phase ;
    rdfs:label "Primary Phase"@en ;
    gui:hasDisplayName "Primary Phase" ;
    gui:hasFormGroup "Microstructure" ;
    gui:hasDisplayOrder 2 ;
    rdfs:comment "Dominant phase in the material"@en .

#################################################################
#    Data Properties - Material Identification
#################################################################

dyn:hasMaterialName rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Material Name"@en ;
    gui:hasDisplayName "Material Name" ;
    gui:hasFormGroup "Identification" ;
    gui:hasDisplayOrder 1 ;
    rdfs:comment "Standard material designation (e.g., Al6061-T6, SS316L)"@en .

dyn:hasMaterialCode rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Material Code"@en ;
    gui:hasDisplayName "Material Code" ;
    gui:hasFormGroup "Identification" ;
    gui:hasDisplayOrder 2 ;
    rdfs:comment "Internal material identification code"@en .

dyn:hasPlottingLabel rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Plotting Label"@en ;
    gui:hasDisplayName "Plotting Label" ;
    gui:hasFormGroup "Identification" ;
    gui:hasDisplayOrder 3 ;
    rdfs:comment "Short label used for plots and visualizations"@en .

dyn:hasSupplier rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Supplier"@en ;
    gui:hasDisplayName "Supplier" ;
    gui:hasFormGroup "Identification" ;
    gui:hasDisplayOrder 4 ;
    rdfs:comment "Material supplier or source"@en .

dyn:hasLotNumber rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Lot Number"@en ;
    gui:hasDisplayName "Lot Number" ;
    gui:hasFormGroup "Identification" ;
    gui:hasDisplayOrder 5 ;
    rdfs:comment "Supplier lot or batch number"@en .

# Extend domain of hasDescription to include Material (defined in core for Specimen)
dyn:hasDescription rdfs:domain dyn:Material .

#################################################################
#    Object Properties - Basic Material Properties (QuantityValue)
#################################################################

dyn:hasDensity rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:Density ;
    rdfs:label "Density"@en ;
    gui:hasDisplayName "Density" ;
    gui:hasFormGroup "PhysicalProperties" ;
    gui:hasDisplayOrder 1 ;
    dyn:hasUnit "unit:GM-PER-CentiM3" ;
    rdfs:comment "Material density"@en .

dyn:hasMeltingPoint rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:MeltingPoint ;
    rdfs:label "Melting Point"@en ;
    gui:hasDisplayName "Melting Point (°C)" ;
    gui:hasFormGroup "ThermalProperties" ;
    gui:hasDisplayOrder 1 ;
    dyn:hasUnit "unit:DEG_C" ;
    rdfs:comment "Melting temperature of the material"@en .

dyn:hasThermalConductivity rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:ThermalConductivity ;
    rdfs:label "Thermal Conductivity"@en ;
    gui:hasDisplayName "Thermal Conductivity (W/m·K)" ;
    gui:hasFormGroup "ThermalProperties" ;
    gui:hasDisplayOrder 2 ;
    dyn:hasUnit "unit:W-PER-M-K" ;
    rdfs:comment "Thermal conductivity coefficient"@en .

dyn:hasSpecificHeat rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:SpecificHeatCapacity ;
    rdfs:label "Specific Heat"@en ;
    gui:hasDisplayName "Specific Heat (J/kg·K)" ;
    gui:hasFormGroup "ThermalProperties" ;
    gui:hasDisplayOrder 3 ;
    dyn:hasUnit "unit:J-PER-KiloGM-K" ;
    rdfs:comment "Specific heat capacity"@en .

dyn:hasWaveSpeed rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range qudt:QuantityValue ;
    qudt:hasQuantityKind qkdv:Velocity ;
    rdfs:label "Wave Speed"@en ;
    gui:hasDisplayName "Elastic Wave Speed (m/s)" ;
    gui:hasFormGroup "MechanicalProperties" ;
    gui:hasDisplayOrder 4 ;
    dyn:hasUnit "unit:M-PER-SEC" ;
    rdfs:comment "Elastic wave propagation speed in material, calculated as C_bar = sqrt(E/ρ). Critical for SHPB bar characterization and pulse timing calculations."@en .

#################################################################
#    Data Properties - Alloy and Composition
#################################################################

dyn:hasAlloyDesignation rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Alloy Designation"@en ;
    gui:hasDisplayName "Alloy Designation" ;
    gui:hasFormGroup "Composition" ;
    gui:hasDisplayOrder 1 ;
    gui:hasValidValues "1100, 2024, 3003, 5052, 6061, 6063, 7075, 316L, 304, 410, Ti-6Al-4V, Other" ;
    rdfs:comment "Standard alloy designation (e.g., 6061, 316L)"@en .

dyn:hasTemper rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Temper"@en ;
    gui:hasDisplayName "Temper Condition" ;
    gui:hasFormGroup "Composition" ;
    gui:hasDisplayOrder 2 ;
    gui:hasValidValues "O, H14, H32, T4, T6, T651, Annealed, Other" ;
    rdfs:comment "Heat treatment condition or temper"@en .

dyn:hasChemicalComposition rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain dyn:Material ;
    rdfs:range xsd:string ;
    rdfs:label "Chemical Composition"@en ;
    gui:hasDisplayName "Chemical Composition" ;
    gui:hasFormGroup "Composition" ;
    gui:hasDisplayOrder 3 ;
    rdfs:comment "Chemical composition description or certificate reference"@en .

