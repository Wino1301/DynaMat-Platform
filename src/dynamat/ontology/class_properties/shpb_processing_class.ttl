# defined in core/annotation_vocabulary.ttl - do not redeclare here.

#################################################################
#    Classes
#################################################################

###  https://dynamat.utep.edu/ontology#PolarityType
dyn:PolarityType rdf:type owl:Class ;
                 rdfs:label "Polarity Type"@en ;
                 rdfs:comment "Enumeration of pulse polarity types (compressive or tensile) used in pulse detection."@en .


###  https://dynamat.utep.edu/ontology#DetectionMetric
dyn:DetectionMetric rdf:type owl:Class ;
                    rdfs:label "Detection Metric"@en ;
                    rdfs:comment "Enumeration of pulse detection metrics (median or peak) used to select the optimal window from multiple k-trial candidates."@en .


###  https://dynamat.utep.edu/ontology#PulseShift
dyn:PulseShift rdf:type owl:Class ;
               rdfs:label "Pulse Shift"@en ;
               rdfs:comment "Integer sample shift applied during pulse alignment. Positive values indicate forward (right) shift, negative values indicate backward (left) shift."@en .


###  https://dynamat.utep.edu/ontology#PulseDetectionParams
dyn:PulseDetectionParams rdf:type owl:Class ;
                         rdfs:label "Pulse Detection Parameters"@en ;
                         rdfs:comment "Configuration parameters for PulseDetector including pulse duration, detection thresholds, polarity, search bounds, and detected window boundaries. Enables reproducible pulse detection and stores the resulting window indices."@en .


###  https://dynamat.utep.edu/ontology#AlignmentParams
dyn:AlignmentParams rdf:type owl:Class ;
                    rdfs:label "Alignment Parameters"@en ;
                    rdfs:comment "Configuration for PulseAligner optimization including fitness weights, linear region fraction, and search bounds for transmitted and reflected pulse shifts."@en .


###  https://dynamat.utep.edu/ontology#EquilibriumMetrics
dyn:EquilibriumMetrics rdf:type owl:Class ;
                       rdfs:label "Equilibrium Metrics"@en ;
                       rdfs:comment "Force and stress equilibrium assessment metrics for SHPB tests. Includes overall metrics (FBC, SEQI, SOI, DSUF) and phase-specific metrics for loading, plateau, and unloading."@en .


###  https://dynamat.utep.edu/ontology#SegmentationParams
dyn:SegmentationParams rdf:type owl:Class ;
                       rdfs:label "Segmentation Parameters"@en ;
                       rdfs:comment "Configuration parameters for pulse segmentation including segment length and noise threshold ratio."@en .


###  https://dynamat.utep.edu/ontology#TukeyWindowParams
dyn:TukeyWindowParams rdf:type owl:Class ;
                      rdfs:label "Tukey Window Parameters"@en ;
                      rdfs:comment "Configuration parameters for Tukey window application including enable flag and alpha parameter."@en .


###  https://dynamat.utep.edu/ontology#DataSeries
dyn:DataSeries rdf:type owl:Class .


###  https://dynamat.utep.edu/ontology#SHPBCompression
dyn:SHPBCompression rdf:type owl:Class .


#################################################################
#    Object Properties - PulseDetectionParams
#################################################################

###  https://dynamat.utep.edu/ontology#appliedToSeries
dyn:appliedToSeries rdf:type owl:ObjectProperty ;
                    rdfs:domain dyn:PulseDetectionParams ;
                    rdfs:range dyn:DataSeries ;
                    rdfs:comment "Links pulse detection parameters to the data series it was applied to (output property, not shown in forms)"@en ;
                    rdfs:label "Applied to Series"@en .


#################################################################
#    Object Properties - PulseShift
#################################################################

###  https://dynamat.utep.edu/ontology#shiftAppliedToSeries
dyn:shiftAppliedToSeries rdf:type owl:ObjectProperty ;
                         rdfs:domain dyn:PulseShift ;
                         rdfs:range dyn:DataSeries ;
                         rdfs:comment "Links pulse shift to the data series it was applied to"@en ;
                         rdfs:label "Shift Applied to Series"@en ;
                         gui:hasDisplayName "Applied to Series" ;
                         gui:hasFormGroup "ShiftReference" ;
                         gui:hasGroupOrder 2 ;
                         gui:hasDisplayOrder 1 ;
                         gui:isReadOnly true .


#################################################################
#    Data Properties - PulseShift
#################################################################

###  https://dynamat.utep.edu/ontology#hasShiftValue
dyn:hasShiftValue rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain dyn:PulseShift ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "Integer shift value in samples. Positive = forward/right, negative = backward/left"@en ;
                  rdfs:label "Shift Value"@en ;
                  gui:hasDisplayName "Shift Value (samples)" ;
                  gui:hasFormGroup "ShiftParameters" ;
                  gui:hasGroupOrder 1 ;
                  gui:hasDisplayOrder 1 ;
                  gui:isReadOnly true .


###  https://dynamat.utep.edu/ontology#hasShiftDirection
dyn:hasShiftDirection rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain dyn:PulseShift ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Direction of shift applied to signal"@en ;
                      rdfs:label "Shift Direction"@en ;
                      gui:hasDisplayName "Shift Direction" ;
                      gui:hasFormGroup "ShiftParameters" ;
                      gui:hasGroupOrder 1 ;
                      gui:hasDisplayOrder 2 ;
                      gui:hasValidValues "forward,backward" ;
                      gui:isReadOnly true .


